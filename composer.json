{
    "name": "meigumi-i/auth",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf"
    ],
    "description": "auth token",
    "autoload": {
        "psr-4": {
            "MeigumiI\\Auth\\": "src/"
        },
        "files": [
            "src/func.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\": "tests"
        }
    },
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "ext-redis": "*",
        "ext-swoole": ">=4.3",
        "hyperf/di": "1.0.*",
        "hyperf/framework": "1.0.*",
        "hyperf/redis": "^1.0",
        "hyperf/utils": "1.0.*",
        "nesbot/carbon": "^2.22"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "hyperf/testing": "1.0.*",
        "phpstan/phpstan": "^0.10.5",
        "swoft/swoole-ide-helper": "dev-master"
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "co-phpunit -c phpunit.xml --colors=always",
        "analyze": "phpstan analyse --memory-limit 300M -l 0 ./src",
        "cs-fix": "php-cs-fixer fix $1"
    },
    "extra": {
        "hyperf": {
            "config": "MeigumiI\\Auth\\ConfigProvider"
        }
    }
}
